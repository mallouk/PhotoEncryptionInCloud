<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utc_4910.photoencryptionincloud" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="utc_4910.screenActivities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="utc_4910.screenActivities.SpillBucketActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.CreateBucketActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.BucketActionActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.GestureActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.LoginActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.CreateAccountActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="utc_4910.screenActivities.ChangePassActivity"
            android:screenOrientation="portrait">
        </activity>



    </application>
</manifest>
